{
    "extends": "airbnb-base",
    "rules": {
        "max-len": [1, 120, 2],
        "quote-props": [1, "consistent-as-needed"],
        "no-cond-assign": [2, "except-parens"],
        "radix": 0,
        "no-unused-vars": "warn",
        "no-plusplus": "off",
        "comma-dangle": "off"
    },
    "env": {
        "jest": true
    }
}